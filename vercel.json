{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/invite",
      "dest": "public/invite.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
